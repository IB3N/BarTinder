{
  "expo": {
    "name": "BarTinder",
    "slug": "BarTinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tumblerSmall.png",
    "splash": {
      "image": "./assets/tumbler.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tumblerSmall.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/tumblerSmall.png"
    }
  }
}
